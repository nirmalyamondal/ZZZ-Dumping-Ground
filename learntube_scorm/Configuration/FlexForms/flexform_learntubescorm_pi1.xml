<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
	<langChildren>1</langChildren>
  </meta>
<sheets>

	<sDEF>
	<ROOT>
	<TCEforms>
		<sheetTitle>General Settings</sheetTitle>
	</TCEforms>
	<type>array</type>
	<el>

		<settings.current_scormtopic_id>
		<TCEforms>
			<label>Select SCORM Cloud Course Object</label>
			<config>
				<type>select</type>
				<items type="array">	
				</items>
				<itemsProcFunc>tx_learntubescorm_flexform->_list_all_SCORM_courses</itemsProcFunc>
				<size>1</size>
				<minitems>1</minitems>
				<maxitems>1</maxitems>
			</config>
		</TCEforms>
		</settings.current_scormtopic_id>

		<settings.launchtext>
			<TCEforms>				
				<label>Launch Link Test Label</label>
				<config>
					<type>input</type>
					<size>100</size>
					<default>Launch Test</default>
				</config>
			</TCEforms>
		</settings.launchtext>

		<settings.show_launch>
		<TCEforms>
			<label>Show Launch Link</label>
			<config>
				<type>check</type>
				<default>1</default>
			</config>
		</TCEforms>
		</settings.show_launch>

		<settings.show_history>
		<TCEforms>
			<label>Show History</label>
			<config>
				<type>check</type>
			</config>
		</TCEforms>
		</settings.show_history>

	</el>
	</ROOT>
	</sDEF>


</sheets>
</T3DataStructure>